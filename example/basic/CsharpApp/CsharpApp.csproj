		<UseWPF>true</UseWPF>

		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\..\csharp\FlutterCsharpRpc.csproj" />
	</ItemGroup>

</Project>
