	<PropertyGroup>
		<OutputType>Exe</OutputType>

		<!-- Using target-framework: 'net7.0-windows' and 'UseWPF' only for the 'MessageBox.Show' example,
			for cross-platform code use target-framework: 'net7.0' and remove 'UseWPF' -->
